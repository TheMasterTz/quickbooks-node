{
  "version": "0.1.14",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TheMasterTz/quickbooks-node"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "quickbookslib-ts",
  "author": "Christian Diaz",
  "module": "dist/.esm.js",
  "size-limit": [
    {
      "path": "dist/.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.4.1",
    "@types/sqlstring": "^2.3.0"
  },
  "dependencies": {
    "axios": "1.4.0",
    "axios-logger": "2.6.2",
    "fs": "^0.0.1-security",
    "lodash": "^4.17.21",
    "sqlstring": "^2.3.3"
  }
}
